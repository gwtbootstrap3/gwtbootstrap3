<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:b="urn:import:com.svenjacobs.gwtbootstrap3.client.ui">

    <b:Row>

        <b:PageHeader text="Navs"/>

        <b:Panel>
            <b:PanelHeader>
                <b:Heading size="3" text="Tabs"/>
            </b:PanelHeader>
            <b:PanelBody>
                <b:NavTabs>
                    <b:ListItem active="true">Item 1</b:ListItem>
                    <b:ListItem>Item 2</b:ListItem>
                    <b:ListItem>Item 3</b:ListItem>
                    <b:ListItem enabled="false">Item 4</b:ListItem>
                    <b:ListItem pull="RIGHT">Pulled right</b:ListItem>
                </b:NavTabs>
            </b:PanelBody>
            <b:PanelFooter>
                <b:Pre addStyleNames="prettyprint lang-xml">
                    &lt;b:NavTabs&gt;\n
                    \s\s&lt;b:ListItem active="true"&gt;Item 1&lt;/b:ListItem&gt;\n
                    \s\s&lt;b:ListItem&gt;Item 2&lt;/b:ListItem&gt;\n
                    \s\s&lt;b:ListItem&gt;Item 3&lt;/b:ListItem&gt;\n
                    \s\s&lt;b:ListItem enabled="false"&gt;Item 4&lt;/b:ListItem&gt;\n
                    \s\s&lt;b:ListItem pull="RIGHT"&gt;Pulled right&lt;/b:ListItem&gt;\n
                    &lt;/b:NavTabs&gt;
                </b:Pre>
            </b:PanelFooter>
        </b:Panel>

        <b:Panel>
            <b:PanelHeader>
                <b:Heading size="3" text="Pills"/>
            </b:PanelHeader>
            <b:PanelBody>
                <b:NavPills>
                    <b:ListItem active="true">Item 1</b:ListItem>
                    <b:ListItem>Item 2</b:ListItem>
                    <b:ListItem>Item 3</b:ListItem>
                    <b:ListItem enabled="false">Item 4</b:ListItem>
                    <b:ListItem pull="RIGHT">Pulled right</b:ListItem>
                </b:NavPills>
                <b:Br/>
                <b:NavPills stacked="true">
                    <b:ListItem active="true">Item 1</b:ListItem>
                    <b:ListItem>Item 2</b:ListItem>
                    <b:ListItem>Item 3</b:ListItem>
                </b:NavPills>
            </b:PanelBody>
            <b:PanelFooter>
                <b:Pre addStyleNames="prettyprint lang-xml">
                    &lt;b:NavPills&gt;\n
                    \s\s&lt;b:ListItem active="true"&gt;Item 1&lt;/b:ListItem&gt;\n
                    \s\s&lt;b:ListItem&gt;Item 2&lt;/b:ListItem&gt;\n
                    \s\s&lt;b:ListItem&gt;Item 3&lt;/b:ListItem&gt;\n
                    \s\s&lt;b:ListItem enabled="false"&gt;Item 4&lt;/b:ListItem&gt;\n
                    \s\s&lt;b:ListItem pull="RIGHT"&gt;Pulled right&lt;/b:ListItem&gt;\n
                    &lt;/b:NavPills&gt;\n
                    \n
                    &lt;b:NavPills stacked="true"&gt;\n
                    \s\s&lt;b:ListItem active="true"&gt;Item 1&lt;/b:ListItem&gt;\n
                    \s\s&lt;b:ListItem&gt;Item 2&lt;/b:ListItem&gt;\n
                    \s\s&lt;b:ListItem&gt;Item 3&lt;/b:ListItem&gt;\n
                    &lt;/b:NavPills&gt;
                </b:Pre>
            </b:PanelFooter>
        </b:Panel>

        <b:Panel>
            <b:PanelHeader>
                <b:Heading size="3" text="Justified Nav"/>
            </b:PanelHeader>
            <b:PanelBody>
                <b:NavTabs justified="true">
                    <b:ListItem active="true">Item 1</b:ListItem>
                    <b:ListItem>Item 2</b:ListItem>
                    <b:ListItem>Item 3</b:ListItem>
                </b:NavTabs>
            </b:PanelBody>
            <b:PanelFooter>
                <b:Pre addStyleNames="prettyprint lang-xml">
                    &lt;b:NavTabs justified="true"&gt;\n
                    \s\s&lt;b:ListItem active="true"&gt;Item 1&lt;/b:ListItem&gt;\n
                    \s\s&lt;b:ListItem&gt;Item 2&lt;/b:ListItem&gt;\n
                    \s\s&lt;b:ListItem&gt;Item 3&lt;/b:ListItem&gt;\n
                    &lt;/b:NavTabs&gt;\n
                </b:Pre>
            </b:PanelFooter>
        </b:Panel>

        <b:Panel>
            <b:PanelHeader>
                <b:Heading size="3" text="Using Dropdowns"/>
            </b:PanelHeader>
            <b:PanelBody>
                <b:NavTabs>
                    <b:ListItem active="true">Item 1</b:ListItem>
                    <b:ListItem>Item 2</b:ListItem>

                    <b:ListDropDown>
                        <b:AnchorButton toggle="DROPDOWN">Dropdown</b:AnchorButton>
                        <b:DropDownMenu>
                            <b:ListItem>Dropdown 1</b:ListItem>
                            <b:ListItem>Dropdown 2</b:ListItem>
                            <b:ListItem>Dropdown 3</b:ListItem>
                        </b:DropDownMenu>
                    </b:ListDropDown>

                    <b:ListItem>Item 3</b:ListItem>
                </b:NavTabs>
                <b:Br/>
                <b:NavPills>
                    <b:ListItem active="true">Item 1</b:ListItem>
                    <b:ListItem>Item 2</b:ListItem>

                    <b:ListDropDown>
                        <b:AnchorButton toggle="DROPDOWN">Dropdown</b:AnchorButton>
                        <b:DropDownMenu>
                            <b:ListItem>Dropdown 1</b:ListItem>
                            <b:ListItem>Dropdown 2</b:ListItem>
                            <b:ListItem>Dropdown 3</b:ListItem>
                        </b:DropDownMenu>
                    </b:ListDropDown>

                    <b:ListItem>Item 3</b:ListItem>
                </b:NavPills>
            </b:PanelBody>
            <b:PanelFooter>
                <b:Pre addStyleNames="prettyprint lang-xml">
                    &lt;b:NavTabs&gt;\n
                    \s\s&lt;b:ListItem active="true"&gt;Item 1&lt;/b:ListItem&gt;\n
                    \s\s&lt;b:ListItem&gt;Item 2&lt;/b:ListItem&gt;\n
                    \n
                    \s\s&lt;b:ListDropDown&gt;\n
                    \s\s\s\s&lt;b:AnchorButton toggle="DROPDOWN"&gt;Dropdown&lt;/b:AnchorButton&gt;\n
                    \s\s\s\s&lt;b:DropDownMenu&gt;\n
                    \s\s\s\s\s\s&lt;b:ListItem&gt;Dropdown 1&lt;/b:ListItem&gt;\n
                    \s\s\s\s\s\s&lt;b:ListItem&gt;Dropdown 2&lt;/b:ListItem&gt;\n
                    \s\s\s\s\s\s&lt;b:ListItem&gt;Dropdown 3&lt;/b:ListItem&gt;\n
                    \s\s\s\s&lt;/b:DropDownMenu&gt;\n
                    \s\s&lt;/b:ListDropDown&gt;\n
                    \n
                    \s\s&lt;b:ListItem&gt;Item 3&lt;/b:ListItem&gt;\n
                    &lt;/b:NavTabs&gt;
                </b:Pre>
            </b:PanelFooter>
        </b:Panel>

    </b:Row>

</ui:UiBinder>