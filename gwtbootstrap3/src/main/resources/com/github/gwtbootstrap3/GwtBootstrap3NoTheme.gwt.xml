<!--
Inherit this module if you want to load up your own custom bootstrap css, just
use <stylesheet src=""> in your gwt.xml. This will put all the needed JS/main CSS
for you.
-->

<module rename-to="GwtBootstrap3NoTheme">
    <inherits name="com.google.gwt.core.Core"/>
    <inherits name="com.google.gwt.resources.Resources"/>
    <inherits name="com.github.gwtbootstrap3.client.NoThemeResources"/>

    <entry-point class="com.github.gwtbootstrap3.client.GwtBootstrap3"/>

    <source path="client"/>
    <source path="shared"/>
</module>